{
  "name": "ember-codemods-telemetry-helpers",
  "version": "0.1.0",
  "description": "Helpers for gathering app telemetry for codemods.",
  "main": "lib/index.js",
  "repository": "https://github.com/ember-codemods/ember-codemods-telemetry-helpers",
  "author": "",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint:js": "eslint .",
    "test": "jest .",
    "release": "release-it"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "execa": "^2.0.3",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "release-it": "^12.3.4",
    "release-it-lerna-changelog": "^1.0.3"
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
    "git-repo-info": "^2.1.0",
    "puppeteer": "^1.19.0",
    "sync-disk-cache": "^1.3.3",
    "walk-sync": "^2.0.2"
  },
  "engines": {
    "node": "8.* || 10.* || >= 12.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "hooks": {
      "before:release": "yarn update-docs"
    },
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
